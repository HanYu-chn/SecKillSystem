<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.seckillSystem.mapper.OrderMapper">
    <insert id="insertOrder">
        insert into orders(id,userId,itemId,itemPrice,amount,totalPrice,promoId)
        values(#{id},#{userId},#{itemId},#{itemPrice},#{amount},#{totalPrice},#{promoId})
    </insert>
</mapper>